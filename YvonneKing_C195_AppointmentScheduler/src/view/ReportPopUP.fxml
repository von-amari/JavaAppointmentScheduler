<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="reportPopUpScreen" prefHeight="448.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ReportPopUPController">
   <children>
      <VBox layoutX="23.0" layoutY="24.0" prefHeight="398.0" prefWidth="354.0" style="-fx-background-color: #748386;">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="600.0" text="Reports By:" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Label>
            <Button fx:id="monthTypeBtn" mnemonicParsing="false" onAction="#onActionMonthType" prefHeight="63.0" prefWidth="149.0" style="-fx-background-color: #9DC7C8;" text="Month and Type" textFill="WHITE">
               <effect>
                  <DropShadow />
               </effect>
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="25.0" left="100.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="contactBtn" mnemonicParsing="false" onAction="#onActionContact" prefHeight="63.0" prefWidth="149.0" style="-fx-background-color: #9DC7C8;" text="Contacts" textFill="WHITE">
               <effect>
                  <DropShadow />
               </effect>
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
               <VBox.margin>
                  <Insets bottom="25.0" left="100.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="thirdReportBtn" mnemonicParsing="false" onAction="#onActionThirdReport" prefHeight="63.0" prefWidth="149.0" style="-fx-background-color: #9DC7C8;" text="Customer" textFill="WHITE">
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
               <effect>
                  <DropShadow />
               </effect>
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#onActionBack" prefHeight="34.0" prefWidth="93.0" style="-fx-background-color: #9DC7C8;" text="Back" textFill="WHITE">
               <opaqueInsets>
                  <Insets bottom="20.0" />
               </opaqueInsets>
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets left="125.0" top="30.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
